{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "components.d.ts",
  ],
  "exclude": ["functions/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "lib": ["es2022"]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ],
  "jsx": "preserve",
}